<?xml version="1.0" encoding="UTF-8"?>
<urlset xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for item in items %}
        <url>
            <loc>{{ item.slug ? absolute_url(path('oswis_org_oswis_web_page', {'slug': item.slug})) : url|default~'/'~item.url|default }}</loc>
            <changefreq>{{ item.changeFrequency|default('daily') }}</changefreq>
            <priority>{{ item.priority|default('0.5000') }}</priority>
        </url>
    {% endfor %}
</urlset>
